# Playwright Service Configuration
# Copy this file to .env and update with your actual service details

# Playwright Service URL (WebSocket endpoint)
# Example: wss://your-service-name.playwright.io
PLAYWRIGHT_SERVICE_URL=

# Access token for authentication (required for Playwright Service)
PLAYWRIGHT_SERVICE_ACCESS_TOKEN=

# Optional: Custom run ID for test session tracking
# If not set, a random UUID will be generated
# PLAYWRIGHT_SERVICE_RUN_ID=my-custom-run-id

# Local testing configuration (when not using service)
HEADLESS=true
SLOW_MO=0